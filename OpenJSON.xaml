<Window x:Class="Lab5.OpenJSON"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab5"
        mc:Ignorable="d"
        Title="OpenJSON" Height="350" Width="500">
    <Grid>
        <TextBox x:Name="FileName" Margin="50,70,50,199">.json</TextBox>

        <Button x:Name="btnOpenJSON" Margin="180,250,180,34" Click="ButtonOpenJSON_Click">
            Open PlayList
        </Button>

        <TextBlock Margin="50,50,261,245"> File name: (Should end with .json)</TextBlock>
    </Grid>
</Window>
