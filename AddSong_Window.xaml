<Window x:Class="Lab5.AddSong_Window"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab5"
        mc:Ignorable="d"
        Title="AddSong_Window" Height="500" Width="800">
    <Grid Height="334" VerticalAlignment="Center">
        <TextBox x:Name="Author" Margin="125,70,125,220"></TextBox>
        <TextBox x:Name="Title" Margin="125,190,125,100"></TextBox>

        <Button x:Name="btnAdd" Margin="255,250,255,34" Click="ButtonAddSong_Click">
            Add song
        </Button>

        <TextBlock Margin="125,50,387,264"> Author:</TextBlock>
        <TextBlock Margin="125,167,387,144"> Title:</TextBlock>
    </Grid>
</Window>
